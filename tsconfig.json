{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "types": ["svelte"],
    "target": "ES2022",
    "module": "ES2022",
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
